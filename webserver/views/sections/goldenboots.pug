extends ../layouts/main.pug

block content 
    div 
        h3 Лучшие бомбардиры сезона с 1990-2023
        p В таблице представлены игроки получавшие награду "Золотая бутса"

    table 
        tr 
            td #{headers[0]}
            td #{headers[1]}
            td #{headers[2]}
        for content in contents 
            tr 
                th #{content.year}
                th #{content.player}
                th #{content.club}